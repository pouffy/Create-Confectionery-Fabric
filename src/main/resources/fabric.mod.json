{
  "schemaVersion": 1,
  "id": "create_confectionery",
  "version": "${version}",
  "name": "Create: Confectionery",
  "description": "Adds sweet treats to the Create mod.",
  "authors": [
    "Furti_Two, PouffyDev"
  ],
  "contact": {
    "sources": "https://github.com/pouffy/Create-Confectionery-Fabric"
  },
  "license": "CC0",
  "icon": "assets/create_confectionery/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.pouffydev.create_confectionery.Confectionery"
    ],
    "fabric-datagen": [
      "com.pouffydev.create_confectionery.ConfectioneryData"
    ],
    "client": [
      "com.pouffydev.create_confectionery.ConfectioneryClient"
    ]
  },
  "mixins": [
    "create_confectionery.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
